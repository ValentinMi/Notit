    "express": "^4.16.2",
    "joi": "^14.3.1",
    "jquery": "^3.4.1",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "mongoose": "^5.6.0",
    "node-cron": "^2.0.3",
